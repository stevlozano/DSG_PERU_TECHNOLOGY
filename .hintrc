{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 18.5",
    "not safari <= 18.5"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "meta-charset-utf-8": "off",
    "meta-viewport": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ]
  }
}